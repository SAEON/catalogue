{
  "name": "@saeon/atlas",
  "version": "6.1.2",
  "description": "Generalized openLayer wrapper for GIS data navigation",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "__GENERAL__": "====================================================================",
    "start": "npm-run-all -p start:server start:dev",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "prettier:staged": "pretty-quick --staged",
    "__SERVER__": "=====================================================================",
    "start:server": "nodemon --exec \"npm-run-all -s prettier:server server:dev\"",
    "prettier:server": "prettier --write \"src/server/**/*.@(js|jsx|css|scss)\"",
    "server:dev": "TZ=utc NODE_ENV=development babel-node src/server/bin/index.js",
    "__DEV__": "=====================================================================",
    "start:dev": "npm-run-all -p onchange dev-server",
    "onchange": "onchange \"src/{_lib,_dev,@saeon}/**/*.@(js|jsx|css|scss)\" -- prettier --write {{changed}}",
    "dev-server": "webpack-dev-server --env.mode=development --open",
    "__PACKAGE__": "=====================================================================",
    "publish-patch": "npm-run-all -s publish:pack increment-patch push-to-npm",
    "publish-minor": "npm-run-all -s publish:pack increment-minor push-to-npm",
    "publish-major": "npm-run-all -s publish:pack increment-major push-to-npm",
    "publish:pack": "webpack --env.mode=production",
    "increment-patch": "scripts/increment-patch.sh",
    "increment-minor": "scripts/increment-minor.sh",
    "increment-major": "scripts/increment-major.sh",
    "push-to-npm": "scripts/push-to-npm.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all -s prettier:staged lint"
    }
  },
  "keywords": [
    "OpenLayers 6",
    "SAEON",
    "Atlas",
    "Map"
  ],
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-md": "^1.14.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "apollo-link-rest": "^0.7.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "graphql-anywhere": "^4.2.6",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "http-proxy-middleware": "^0.20.0",
    "morgan": "^1.9.1",
    "ol": "^6.1.1",
    "react-apollo": "^3.1.3",
    "react-beautiful-dnd": "^12.2.0",
    "url": "^0.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/saeon-atlas/issues"
  },
  "homepage": "https://github.com/SAEONData/saeon-atlas"
}
