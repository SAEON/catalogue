{
  "name": "@saeon/atlas",
  "version": "6.1.1",
  "description": "Generalized openLayer wrapper for GIS data navigation",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm-run-all -p onchange dev-server",
    "onchange": "onchange \"./**/*.@(js|jsx|css|scss)\" -- prettier --write {{changed}}",
    "dev-server": "webpack-dev-server --env.mode=development --open",
    "prettier": "prettier --write \"./**/*.@(js|jsx|css|scss)\"",
    "publish-patch": "npm-run-all -s build increment-patch push-to-npm",
    "publish-minor": "npm-run-all -s build increment-minor push-to-npm",
    "publish-major": "npm-run-all -s build increment-major push-to-npm",
    "increment-patch": "./scripts/increment-patch.sh",
    "increment-minor": "./scripts/increment-minor.sh",
    "increment-major": "./scripts/increment-major.sh",
    "push-to-npm": "./scripts/push-to-npm.sh",
    "build": "npm-run-all pack",
    "rm-dist": "rm -rf ./dist",
    "pack": "webpack --env.mode=production"
  },
  "keywords": [
    "OpenLayers",
    "SAEON",
    "Atlas",
    "Map"
  ],
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-md": "^1.14.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "ol": "^6.1.1",
    "react-beautiful-dnd": "^12.2.0",
    "xml2js": "^0.4.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/saeon-atlas/issues"
  },
  "homepage": "https://github.com/SAEONData/saeon-atlas"
}
