  "scripts": {
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|ts|tsx|css|scss)\" \"../../packages/snap-menus/src/**/*.@(js|jsx|ts|tsx)\" \"../../packages/logger/src/**/*.@(js|jsx|ts|tsx)\"  \"../../packages/pkce-client/src/**/*.@(js|jsx|ts|tsx)\" \"../../packages/ol-react/src/**/*.@(js|jsx|ts|tsx)\" -- prettier --write {{changed}} &  webpack serve --env development --port 4001 --open",
