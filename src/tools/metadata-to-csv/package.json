{
  "name": "metadata-to-csv",
  "version": "1.0.0",
  "description": "Convert metadata downloaded from the SAEON catalogue to CSV format",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "npm run prettier && node --trace-warnings --experimental-json-modules src/index.js",
    "prettier": "prettier --write \"./**/*.@(js|jsx|mjs|cjs|graphql)\"",
    "ncu": "ncu",
    "ncu -u": "ncu -u"
  },
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "csv-stringify": "^5.6.2",
    "objects-to-csv": "^1.3.6"
  },
  "devDependencies": {
    "npm-check-updates": "^11.2.1",
    "prettier": "^2.2.1"
  }
}
