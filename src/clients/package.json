{
  "name": "@catalogue/client",
  "version": "3.0.0",
  "description": "The South African Environmental Observation Network (SAEON) Open Data Platform (ODP) is a metadata repository that facilitates the publication, discovery, dissemination, and preservation of earth observation and environmental data in South Africa. SAEON is a long-term environmental observation and research facility of the National Research Foundation (NRF).",
  "keywords": [
    "GIS",
    "OGC atlas",
    "SAEON Data Portal",
    "Catalogue",
    "SAEON",
    "ULWAZI"
  ],
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack --config webpack/index.js",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|ts|tsx|css|scss)\" \"../packages/snap-menus/dist/**/*.@(js|jsx|ts|tsx)\" \"../packages/logger/dist/**/*.@(js|jsx|ts|tsx)\" \"../packages/ol-react/dist/**/*.@(js|jsx|ts|tsx)\" -- prettier --write {{changed}} & NODE_ENV=development webpack serve --port 3001 --open --config webpack/index.js",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.{js,jsx}\"",
    "ncu": "ncu -x ol",
    "ncu -u": "ncu -u -x ol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEON/catalogue.git"
  },
  "bugs": {
    "url": "https://github.com/SAEON/catalogue/issues"
  },
  "homepage": "https://github.com/SAEON/catalogue/tree/master/src/@saeon/client#readme",
  "author": "zach@saeon.ac.za,steven@saeon.ac.za",
  "license": "MIT",
