{
  "name": "@saeon/ol-react-dev",
  "version": "0.0.1",
  "description": "Client app for testing ol-react component - no styling libraries please!",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm-run-all -p onchange dev-server",
    "onchange": "onchange \"src/**/*.@(js|jsx|css|scss)\" -- prettier --write {{changed}}",
    "dev-server": "webpack-dev-server --env.mode=development --env.entry=src/_test/index.jsx --env.port=3002 --open"
  },
