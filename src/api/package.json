{
  "name": "@catalogue/api",
  "version": "3.0.0",
  "description": "Node.js/Express.js server supporting the @saeon/clients applications",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --ext js,graphql,json --watch ./src --exec 'prettier --write \"src/**/*.@(js|json|graphql)\" && TZ=utc NODE_TLS_REJECT_UNAUTHORIZED=0 node --trace-warnings --experimental-json-modules src'",
    "start:prod": "TZ=utc node --no-warnings --experimental-json-modules src",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.{js,mjs,cjs}\"",
    "ncu": "ncu -x es7.14,graphql",
    "ncu -u": "ncu -u -x es7.14,graphql"
  },
  "keywords": [
    "@saeon/api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEON/catalogue.git"
  },
  "bugs": {
    "url": "https://github.com/SAEON/catalogue/issues"
  },
  "homepage": "https://github.com/SAEON/catalogue/tree/master/src/@saeon/api#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/eslint-plugin": "^7.14.5",
    "eslint": "^8.1.0",
    "nodemon": "^2.0.14",
    "npm-check-updates": "^12.0.0",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "@koa/router": "^10.1.1",
    "@saeon/logger": "^6.2.15",
    "apollo-server-core": "^3.4.1",
    "apollo-server-koa": "3.4",
    "archiver": "^5.3.0",
    "base64url": "^3.0.1",
    "bcrypt": "^5.0.1",
    "btoa": "^1.2.1",
    "combined-stream": "^1.0.8",
    "cookie": "^0.4.1",
    "cron": "^1.8.2",
    "dataloader": "^2.0.0",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "es7.14": "npm:@elastic/elasticsearch@^7.14.1",
    "form-data": "^4.0.0",
    "graphql": "^15.6.1",
    "graphql-type-json": "^0.3.2",
    "JSONStream": "^1.3.5",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.1.0",
    "koa-passport": "^4.1.4",
    "koa-session": "^6.2.0",
    "mongodb": "^4.1.4",
    "nanoid": "^3.1.30",
    "node-fetch": "^3.0.0",
    "object-hash": "^2.2.0",
    "openid-client": "^5.0.2",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.7.1",
    "pg-cursor": "^2.7.1",
    "pg-query-stream": "^4.2.1",
    "rimraf": "^3.0.2",
    "sift": "^15.0.0",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2",
    "wkt": "^0.1.1"
  },
  "author": "zach@saeon.ac.za, steven@saeon.ac.za",
  "license": "MIT"
}
