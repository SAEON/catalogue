{
  "name": "@saeon/catalogue-client",
  "version": "0.0.1",
  "description": "Website for navigating SAEON's metadata catalogue",
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack --env.output=dist",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|css|scss)\" \"../catalogue-search/src/**/*.@(js|jsx)\" \"../snap-menus/src/**/*.@(js|jsx)\" \"../logger/src/**/*.@(js|jsx)\"  \"../pkce-client/src/**/*.@(js|jsx)\" -- prettier --write {{changed}} & NODE_ENV=development webpack-dev-server --port 3001 --env.output=dist --open",
    "check-package-updates": "ncu",
    "update-package-dependencies": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "keywords": [
    "SAEON",
    "metadata-catalogue"
  ],
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SAEONData/saeon-atlas/issues"
  },
  "homepage": "https://github.com/SAEONData/saeon-atlas#readme",
  "dependencies": {
    "@babel/preset-env": "^7.10.2",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@saeon/catalogue-search": "^5.0.5",
    "@saeon/logger": "^3.1.11",
    "@saeon/pkce-client": "^0.5.6",
    "@svgr/webpack": "^5.4.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "typeface-roboto": "0.0.75",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "devDependencies": {
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "webpack-dev-server": "^3.11.0"
  }
}
