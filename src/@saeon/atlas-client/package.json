{
  "name": "@saeon/atlas-client",
  "version": "0.0.15",
  "description": "An interactive atlas for interacting with spatial data - specifically SAEON's metadata store, although it should also be a generally useful means of interacting with any OGC-compliant resource.",
  "keywords": [
    "@saeon/atlas-client"
  ],
  "scripts": {
    "build": "rimraf ./dist && webpack --env.mode=production --env.output=dist --env.entry=src/index.jsx",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|css|scss)\" -- prettier --write {{changed}} & webpack-dev-server --port 3001 --env.mode=development --env.output=dist --env.entry=src/index.jsx --open",
    "check-package-updates": "ncu",
    "update-packages": "ncu -u"
  },
  "author": "zach@saeon.ac.za,steven@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "@material-ui/pickers": "^3.2.10",
    "@saeon/catalogue-search": "^1.1.5",
    "@saeon/ol-react": "^7.1.5",
    "@svgr/webpack": "^5.3.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "date-fns": "^2.12.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "html2canvas": "^1.0.0-rc.5",
    "nanoid": "^3.1.3",
    "node-sass": "^4.13.1",
    "ol": "^6.3.1",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.3.1",
    "react-json-tree": "^0.11.2",
    "react-resizable": "^1.10.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-virtualized": "^9.21.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "typeface-roboto": "0.0.75",
    "url": "^0.11.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "onchange": "^6.1.0",
    "prettier": "^2.0.4",
    "webpack-dev-server": "^3.10.3"
  }
}
