{
  "name": "@saeon/atlas-client",
  "version": "0.0.67",
  "description": "An interactive atlas for interacting with spatial data - specifically SAEON's metadata store, although it should also be a generally useful means of interacting with any OGC-compliant resource.",
  "keywords": [
    "@saeon/atlas-client"
  ],
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack --env.output=dist --env.entry=src/index.jsx",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|css|scss)\" \"../snap-menus/src/**/*.@(js|jsx|css|scss)\" -- prettier --write {{changed}} & NODE_ENV=development webpack-dev-server --port 3001 --env.output=dist --env.entry=src/index.jsx --open",
    "check-package-updates": "ncu --reject \"@date-io/date-fns\"",
    "update-packages": "ncu -u --reject \"@date-io/date-fns\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "author": "zach@saeon.ac.za,steven@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.44",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@babel/runtime-corejs3": "^7.9.6",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "@material-ui/pickers": "^3.2.10",
    "@saeon/catalogue-search": "^4.1.2",
    "@saeon/ol-react": "^8.0.1",
    "@saeon/snap-menus": "^2.0.7",
    "@svgr/webpack": "^5.4.0",
    "babel-loader": "^8.1.0",
    "clsx": "^1.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "date-fns": "^2.12.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^6.0.0",
    "graphql": "^15.0.0",
    "html-webpack-plugin": "^4.3.0",
    "html2canvas": "^1.0.0-rc.5",
    "nanoid": "^3.1.5",
    "node-sass": "^4.14.1",
    "ol": "^6.3.1",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-device-detect": "^1.12.1",
    "react-dom": "^16.13.1",
    "react-json-tree": "^0.11.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-virtualized": "^9.21.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "subscriptions-transport-ws": "^0.9.16",
    "typeface-roboto": "0.0.75",
    "url": "^0.11.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "webpack-dev-server": "^3.10.3"
  }
}
