{
  "name": "@saeon/ol-react",
  "version": "6.1.9",
  "description": "React wrapper for the open-source OpenLayers JavaScript library, envisioned as a generally useful, opinionated means of interacting with OpenLayers within a React.js application",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "scripts": {
    "pack": "webpack --env.mode=production --env.output=src/@saeon/ol-react/dist/ --env.entry=src/@saeon/ol-react/src/index.js",
    "publish:patch": "npm-run-all -s pack increment:patch push-to-npm",
    "publish-minor": "npm-run-all -s pack increment-minor push-to-npm",
    "publish-major": "npm-run-all -s pack increment-major push-to-npm",
    "increment:patch": "../../../scripts/increment-patch.sh",
    "increment-minor": "../../../scripts/increment-minor.sh",
    "increment-major": "../../../scripts/increment-major.sh",
    "push-to-npm": "../../../scripts/push-to-npm.sh -p @saeon/ol-react"
  },
  "keywords": [
    "@saeon/ol-react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/saeon-atlas/issues"
  },
  "homepage": "https://github.com/SAEONData/saeon-atlas/tree/master/src/%40saeon/ol-react",
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {}
}
