{
  "name": "@saeon/ol-react",
  "version": "6.1.2",
  "description": "React wrapper for the open-source OpenLayers JavaScript library, envisioned as a generally useful, opinionated means of interacting with OpenLayers within a React.js application",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "scripts": {
    "publish-patch": "npm-run-all -s publish:pack increment-patch push-to-npm",
    "publish-minor": "npm-run-all -s publish:pack increment-minor push-to-npm",
    "publish-major": "npm-run-all -s publish:pack increment-major push-to-npm",
    "publish:pack": "webpack --env.mode=production --env.entry=src/@saeon/ol-react/index.jsx",
    "increment-patch": "scripts/increment-patch.sh",
    "increment-minor": "scripts/increment-minor.sh",
    "increment-major": "scripts/increment-major.sh",
    "push-to-npm": "scripts/push-to-npm.sh"
  },
  "keywords": [
    "@saeon/ol-react"
  ],
  "author": "zach@saeon.ac.za",
  "license": "MIT"
}
