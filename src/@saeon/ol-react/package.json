{
  "name": "@saeon/ol-react",
  "version": "9.0.4",
  "description": "React wrapper for the open-source OpenLayers JavaScript library, envisioned as a generally useful, opinionated means of interacting with OpenLayers within a React.js application",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16",
    "ol": ">=6"
  },
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack --env.output=dist --env.entry=src/index.js",
    "publish:patch": "npm run build && ../../../scripts/publish.sh -p  @saeon/ol-react -s patch",
    "publish:minor": "npm run build && ../../../scripts/publish.sh -p  @saeon/ol-react -s minor",
    "publish:major": "npm run build && ../../../scripts/publish.sh -p  @saeon/ol-react -s major",
    "prettier:watch": "onchange \"src/**/*.@(js|jsx|css|scss)\" -- prettier --write {{changed}}",
    "check-package-updates": "ncu",
    "update-packages": "ncu -u"
  },
  "keywords": [
    "@saeon/ol-react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/saeon-atlas/issues"
  },
  "homepage": "https://github.com/SAEONData/saeon-atlas/tree/master/src/@saeon/ol-react",
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "ol": "^6.3.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {}
}
