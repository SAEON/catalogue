{
  "name": "@saeon/snap-menus",
  "version": "0.0.1",
  "description": "Draggable, snappable menus",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "scripts": {
    "build": "rimraf ./dist && webpack --env.mode=production --env.output=dist --env.entry=src/index.js",
    "publish:patch": "npm run build && ../../../scripts/publish.sh -p  @saeon/snap-menus -s patch",
    "publish:minor": "npm run build && ../../../scripts/publish.sh -p  @saeon/snap-menus -s minor",
    "publish:major": "npm run build && ../../../scripts/publish.sh -p  @saeon/snap-menus -s major",
    "prettier:watch": "onchange \"src/**/*.@(js|jsx|css|scss)\" -- prettier --write {{changed}}",
    "check-package-updates": "ncu",
    "update-packages": "ncu -u"
  },
  "keywords": [
    "@saeon/snap-menus"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/saeon-atlas/issues"
  },
  "homepage": "https://github.com/SAEONData/saeon-atlas/tree/master/src/%40saeon/snap-menus",
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {}
}
