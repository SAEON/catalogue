{
  "name": "reporting",
  "version": "0.0.1",
  "description": "This project generates reports on progress in terms of work done on the SAEON-Atlas repository by interacting with the Github API",
  "type": "module",
  "scripts": {
    "start": "rimraf .build && nodemon --watch ./src --exec 'prettier --write \"src/**/*.@(js|json|graphql)\" && NODE_ENV=development TZ=utc node src/index.js'",
    "check-package-updates": "ncu",
    "update-package-dependencies": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/saeon-atlas/issues"
  },
  "homepage": "https://github.com/SAEONData/saeon-atlas/tree/master/src/reporting",
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.10.3",
    "@saeon/logger": "^3.1.11",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-fetch": "^0.7.0",
    "apollo-link-http": "^1.5.17",
    "copyfiles": "^2.3.0",
    "csv-stringify": "^5.5.0",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
