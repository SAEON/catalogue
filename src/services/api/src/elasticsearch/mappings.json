{
  "dynamic_templates": [
    {
      "booleans": {
        "mapping": {
          "type": "boolean"
        },
        "match_mapping_type": "boolean"
      }
    },
    {
      "integers": {
        "mapping": {
          "type": "long"
        },
        "match_mapping_type": "long"
      }
    },
    {
      "doubles": {
        "mapping": {
          "type": "double"
        },
        "match_mapping_type": "double"
      }
    },
    {
      "dates": {
        "mapping": {
          "type": "date",
          "fields": {
            "raw": {
              "ignore_above": 256,
              "type": "keyword",
              "normalizer": "keyword_trimmed_lower"
            }
          }
        },
        "match_mapping_type": "string",
        "match": "gte|lte"
      }
    },
    {
      "spatial": {
        "mapping": {
          "type": "keyword",
          "normalizer": "keyword_trimmed_lower",
          "fields": {
            "geo_shape": {
              "type": "geo_shape"
            }
          }
        },
        "match_mapping_type": "string",
        "match": "geoLocationBox*"
      }
    },
    {
      "publicationYear": {
        "mapping": {
          "type": "long"
        },
        "match_mapping_type": "string",
        "match": "publicationYear"
      }
    },
    {
      "name": {
        "mapping": {
          "type": "text",
          "fields": {
            "raw": {
              "ignore_above": 256,
              "type": "keyword",
              "normalizer": "keyword_trimmed_lower"
            }
          }
        },
        "match_mapping_type": "string",
        "match": "name"
      }
    },
    {
      "subject": {
        "mapping": {
          "type": "text",
          "fields": {
            "raw": {
              "ignore_above": 256,
              "type": "keyword",
              "normalizer": "keyword_trimmed_lower"
            }
          }
        },
        "match_mapping_type": "string",
        "match": "subject"
      }
    },
    {
      "DOIS": {
        "mapping": {
          "type": "text",
          "fields": {
            "raw": {
              "ignore_above": 256,
              "type": "keyword",
              "normalizer": "keyword_trimmed_lower"
            }
          }
        },
        "match_mapping_type": "string",
        "match": "identifier"
      }
    },
    {
      "strings": {
        "mapping": {
          "analyzer": "saeon_text_fields",
          "type": "text",
          "fields": {
            "raw": {
              "ignore_above": 256,
              "type": "keyword",
              "normalizer": "keyword_trimmed_lower"
            }
          }
        },
        "match_mapping_type": "string",
        "unmatch": "publicationYear|identifier|name|subject|gte|lte"
      }
    }
  ]
}
