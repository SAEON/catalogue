{
  "name": "@saeon/client",
  "version": "0.1.3",
  "description": "Browser-based atlas for searching OGC-compliant spatial data repositories",
  "keywords": [
    "@saeon/client",
    "GIS",
    "OGC-compliant",
    "Atlas",
    "SAEON",
    "ULWAZI"
  ],
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|css|scss)\" \"../../packages/catalogue-search/src/**/*.@(js|jsx)\" \"../../packages/snap-menus/src/**/*.@(js|jsx)\" \"../../packages/logger/src/**/*.@(js|jsx)\"  \"../../packages/pkce-client/src/**/*.@(js|jsx)\" -- prettier --write {{changed}} & NODE_ENV=development webpack-dev-server --port 3001 --open",
    "check-package-updates": "ncu --reject @date-io/date-fns,@saeon/catalogue-search",
    "update-package-dependencies": "ncu -u --reject @date-io/date-fns,@saeon/catalogue-search"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/saeon-atlas.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/saeon-atlas/issues"
  },
  "homepage": "https://github.com/SAEONData/saeon-atlas/tree/master/src/@saeon/client#readme",
  "author": "zach@saeon.ac.za,steven@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.0.2",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@babel/preset-env": "^7.10.4",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^5.0.0-alpha.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@saeon/catalogue-search": "5.3.0",
    "@saeon/logger": "^3.1.12",
    "@saeon/ol-react": "^9.0.9",
    "@saeon/pkce-client": "0.5.8",
    "@saeon/quick-form": "1.0.4",
    "@saeon/snap-menus": "^3.2.5",
    "@svgr/webpack": "^5.4.0",
    "babel-loader": "^8.1.0",
    "clsx": "^1.1.1",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^3.6.5",
    "css-loader": "^4.0.0",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^2.0.0",
    "file-loader": "^6.0.0",
    "graphql": "^15.3.0",
    "html-webpack-plugin": "^4.3.0",
    "html2canvas": "^1.0.0-rc.5",
    "minisearch": "^2.4.1",
    "nanoid": "^3.1.11",
    "node-sass": "^4.14.1",
    "ol": "^6.4.0",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-device-detect": "^1.13.1",
    "react-dom": "^16.13.1",
    "react-json-tree": "^0.11.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-virtualized": "^9.21.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "subscriptions-transport-ws": "^0.9.17",
    "typeface-roboto": "0.0.75",
    "url": "^0.11.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "devDependencies": {
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "webpack-dev-server": "^3.11.0"
  }
}
