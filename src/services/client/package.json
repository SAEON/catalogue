{
  "name": "@catalogue/client",
  "version": "1.0.26",
  "description": "Browser-based client for searching SAEON's metadata repository, and for previewing related, OGC-compliant spatial data",
  "keywords": [
    "@saeon/client",
    "GIS",
    "OGC-compliant",
    "Catalogue",
    "SAEON",
    "ULWAZI"
  ],
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|css|scss)\" \"../../packages/catalogue-search/src/**/*.@(js|jsx)\" \"../../packages/snap-menus/src/**/*.@(js|jsx)\" \"../../packages/logger/src/**/*.@(js|jsx)\"  \"../../packages/pkce-client/src/**/*.@(js|jsx)\" \"../../packages/ol-react/src/**/*.@(js|jsx)\" -- prettier --write {{changed}} & NODE_ENV=development webpack-dev-server --port 3001 --open",
    "ncu": "ncu --reject @date-io/date-fns,webpack,webpack-cli",
    "ncu -u": "ncu -u --reject @date-io/date-fns,webpack,webpack-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/catalogue.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/catalogue/issues"
  },
  "homepage": "https://github.com/SAEONData/catalogue/tree/master/src/@saeon/client#readme",
  "author": "zach@saeon.ac.za,steven@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@saeon/logger": "^6.1.2",
    "@saeon/ol-react": "^10.1.2",
    "@saeon/pkce-client": "0.6.3",
    "@saeon/quick-form": "3.1.2",
    "@saeon/snap-menus": "^5.1.2",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "clsx": "^1.1.1",
    "copy-webpack-plugin": "^6.3.2",
    "core-js": "^3.8.0",
    "css-loader": "^5.0.1",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "file-loader": "^6.2.0",
    "graphql": "^15.4.0",
    "html-webpack-plugin": "^4.5.0",
    "mdi-react": "^7.4.0",
    "minisearch": "^3.0.2",
    "nanoid": "^3.1.18",
    "node-sass": "^5.0.0",
    "notistack": "^1.0.2",
    "object-hash": "^2.0.3",
    "ol": "^6.4.3",
    "react": "^17.0.1",
    "react-ace": "^9.2.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-contexify": "^5.0.0",
    "react-device-detect": "^1.14.0",
    "react-dom": "^17.0.1",
    "react-measure": "^2.5.2",
    "react-router": "^5.2.0",
