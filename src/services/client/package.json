{
  "name": "@catalogue/client",
  "version": "2.0.3",
  "description": "Browser-based client for searching SAEON's metadata repository, and for previewing related, OGC-compliant spatial data",
  "keywords": [
    "@saeon/client",
    "GIS",
    "OGC atlas",
    "Catalogue",
    "SAEON",
    "ULWAZI"
  ],
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|ts|tsx|css|scss)\" \"../../packages/snap-menus/src/**/*.@(js|jsx|ts|tsx)\" \"../../packages/logger/src/**/*.@(js|jsx|ts|tsx)\"  \"../../packages/pkce-client/src/**/*.@(js|jsx|ts|tsx)\" \"../../packages/ol-react/src/**/*.@(js|jsx|ts|tsx)\" -- prettier --write {{changed}} & NODE_ENV=development webpack serve --port 3001 --open",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.{js,jsx}\"",
    "ncu": "ncu",
    "ncu -u": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/catalogue.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/catalogue/issues"
  },
  "homepage": "https://github.com/SAEONData/catalogue/tree/master/src/@saeon/client#readme",
  "author": "zach@saeon.ac.za,steven@saeon.ac.za",
  "license": "MIT",
