{
  "name": "@saeon/snap-menus",
  "version": "5.2.0",
  "description": "Draggable, snappable menus",
  "main": "src/index.js",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./src/index.jsx",
      "require": "./dist/index.js"
    },
    "./provider": {
      "import": "./src/provider.jsx",
      "require": "./dist/provider.js"
    }
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "@material-ui/core": ">=4",
    "@material-ui/icons": ">=4",
    "clsx": ">=1"
  },
  "scripts": {
    "build": "rimraf ./dist && rollup --config rollup.config.js",
    "publish:patch": "npm run build && TC=utc node ../../../scripts/publish.js -p  @saeon/snap-menus -s patch",
    "publish:minor": "npm run build && TC=utc node ../../../scripts/publish.js -p  @saeon/snap-menus -s minor",
    "publish:major": "npm run build && TC=utc node ../../../scripts/publish.js -p  @saeon/snap-menus -s major",
    "ncu": "ncu",
    "ncu -u": "ncu -u"
  },
  "keywords": [
    "@saeon/snap-menus"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/catalogue.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/catalogue/issues"
  },
  "homepage": "https://github.com/SAEONData/catalogue/tree/master/src/@saeon/snap-menus",
  "author": "zach@saeon.ac.za, steven@saeon.ac.za",
  "license": "MIT",
  "dependencies": {
    "react-draggable": "^4.4.3",
    "react-resizable": "^1.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "npm-check-updates": "^10.2.5",
    "postcss": "^8.2.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^4.0.0"
  }
}
