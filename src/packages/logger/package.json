{
  "name": "@saeon/logger",
  "version": "6.2.6",
  "description": "Simple package to add timestamps to console.* statements, as well as a means to extend the console object for alternative logging methods",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./logToGql": "./dist/log-to-graphql.js",
    "./logToHttp": "./dist/log-to-graphql.js"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@apollo/client": ">=3",
    "date-fns": "^2.19.0"
  },
  "scripts": {
    "build": "rimraf ./dist && rollup --config rollup.config.js",
    "publish:patch": "npm run build && TC=utc node ../../../scripts/publish.js -p  @saeon/logger -s patch",
    "publish:minor": "npm run build && TC=utc node ../../../scripts/publish.js -p  @saeon/logger -s minor",
    "publish:major": "npm run build && TC=utc node ../../../scripts/publish.js -p  @saeon/logger -s major",
    "ncu": "ncu",
    "ncu -u": "ncu -u"
  },
  "keywords": [
    "@saeon/logger"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAEONData/catalogue.git"
  },
  "bugs": {
    "url": "https://github.com/SAEONData/catalogue/issues"
  },
  "homepage": "https://github.com/SAEONData/catalogue/tree/master/src/@saeon/logger",
  "author": "zach@saeon.ac.za",
  "license": "MIT",
  "devDependencies": {
    "@apollo/client": "^3.2.1",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "core-js": "^3.9.1",
    "date-fns": "^2.19.0",
    "npm-check-updates": "^11.3.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.4"
  }
}
