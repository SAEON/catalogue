// Issued refers to the metadata data stamp (It shouldn’t be important to users). The publication date should be under “PublicationYear”


POST /saeon-odp-4-2/_search
{
  "query": {
    "range": {
      "dates.date": {
        "gte" : "2001-01-01",
        "lte" : "20019-12-31",
        "relation" : "within"
      }
    }
  }
}


POST /saeon-odp-4-2/_search
{
  "_source": {
    "includes": [
      "dates.*"
    ]
  },
  "query": {
    "range": {
      "dates.date.gte": {
        "lte":  "2005"
      }
    }
  }
}


POST /saeon-odp-4-2/_search
{
  "_source": {
    "includes": [
      "dates.*",
      "identifier",
      "alternateIdentifiers.*",
      "titles.*",
      "geoLocations.*"
    ]
  },
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "dates.date.gte": {
              "lte": 2012,
              "gte": 2012
            }
          }
        },
        {
          "range": {
            "dates.date.lte": {
              "lte": 2012,
              "gte": 2012
            }
          }
        },
        {
          "terms": {
            "dates.dateType": [
              "valid",
              "Collected"
            ]
          }
        }
      ]
    }
  }
}

POST /saeon-odp-4-2/_search
{
  "_source": {
    "includes": [
      "geoLocations.*"
    ]
  },
  "query": {
    "bool": {
      "filter": {
        "geo_shape": {
          "geoLocations.geoLocationBox": {
            "shape": {
              "type": "polygon",
              "coordinates": [
                [
                  [
                    -26.528445,
                    27.702679
                  ],
                  [
                    -26.528445,
                    28.215037
                  ],
                  [
                    -25.900638,
                    28.215037
                  ],
                  [
                    -25.900638,
                    27.702679
                  ],
                  [
                    -26.528445,
                    27.702679
                  ]
                ]
              ]
            },
            "relation": "within"
          }
        }
      }
    }
  }
}

POST /saeon-odp-4-2/_search
{
  "_source": {
    "includes": [
      "geoLocations.*"
    ]
  },
  "query": {
    "exists": {
      "field": "geoLocations.geoLocationBox"
    }
  }
}

POST /saeon-odp-4-2/_search
{
  "_source": {
    "includes": ["dates.*"]
  },
  "query": {
    "terms": {
      "dates.dateType": ["valid", "Collected"]
      }
    }
  }
}


POST /saeon-odp-4-2/_search
{
  "_source": {
    "includes": ["dates.*"]
  },
  "query": {
    "match": {
      "dates.dateType": {
        "query": "Valid"
      }
    }
  }
}



POST /saeon-odp-4-2/_search
{
  "_source": {
    "includes": ["dates.*"]
  },
  "query": {
    "exists": {
      "field": "dates.dateType*"
      }
    }
  }
}